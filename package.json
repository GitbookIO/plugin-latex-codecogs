{
    "name": "gitbook-plugin-latex-codecogs",
    "description": "Display math equations using latex.codecogs.com",
    "main": "index.js",
    "version": "1.0.1",
    "license": "Apache-2.0",
    "homepage": "https://github.com/GitbookIO/plugin-latex-codecogs",
    "keywords": ["math", "latex"],
    "engines": {
        "gitbook": ">=2.4.3"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/GitbookIO/plugin-latex-codecogs.git"
    },
    "bugs": {
        "url": "https://github.com/GitbookIO/plugin-latex-codecogs/issues"
    },
    "gitbook": {
        "properties": {
            "type": {
                "type": "string",
                "default": "gif",
                "title": "Image file type to use",
                "enum": ["gif", "svg"]
            }
        }
    }
}
